#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main()
{
    int n;
    cin >> n;
    vector<int> arr(n);
    vector<int> answer(n,0);
    for(int i=0; i<n; i++)
    {
        cin >> arr[i];
    }

    int maxNum = 0;

    for(int i=0; i<n; i++)
    {
        int num = arr[i];
        for(int j=i+1; j<n; j++)
        {
            answer[i] = max(num,num+arr[j]);
        }
    }
}