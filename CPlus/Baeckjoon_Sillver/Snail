#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, num;
    cin >> n >> num;

    int dx[] = { -1, 0, 1, 0 };
    int dy[] = { 0, 1, 0, -1 };
    vector<vector<int>> matrix(n, vector<int>(n, 0));
    int number = 2;
    int x = n / 2;
    int y = n / 2;
    matrix[x][y] = 1;
    int idx = 1;
    int i = 0; 

    vector<int> answer = { x + 1, y + 1 };
    while (x != 0 || y != 0) 
    {
        int flag = 0;
        for (int k = 0; k < 2; ++k) 
        {
            for (int j = 0; j < idx; ++j)
            {
                x += dx[i];
                y += dy[i];
                matrix[x][y] = number;
                if (number == num) 
                {
                    answer = { x + 1, y + 1 };
                }
                if (x == 0 && y == 0) 
                {
                    flag = 1;
                    break;
                }
                number += 1;
            }
            if (flag == 1)
             break;
            i = (i + 1) % 4;
        }
        idx += 1;
    }

    for (int i = 0; i < n; ++i) 
    {
        for (int j = 0; j < n; ++j) 
        {
            cout << matrix[i][j] << " ";
        }
        cout << endl;
    }

    cout << answer[0] << " " << answer[1];

    return 0;
}