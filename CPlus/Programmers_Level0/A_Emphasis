#include <string>
#include <vector>
#include <cctype>
using namespace std;

string solution(string myString) 
{
    string answer = "";
    for(char ch : myString)
    {
        if(ch == 'a' || ch == 'A')
            answer += 'A';
        else
            answer += tolower(ch);
    }
    return answer;
}