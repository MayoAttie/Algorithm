#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main()
{
    int N, M;
    cin >> N >> M;
    
	string answer = "";
	for (int i = 0; i < N; i++) 
		answer = answer + to_string(N);
	if (answer.size() > M)
		cout << answer.substr(0, M);
	else 
		cout << answer;

    return 0;
}