#include <iostream>
#include <cmath>

using namespace std;

int main() 
{
    int n, w, h;
    cin >> n >> w >> h;

    double len = sqrt(w * w + h * h);

    for (int i = 0; i < n; i++) 
    {
        int a;
        cin >> a;

        if (len >= a)
            cout << "DA\n";
        else 
            cout << "NE\n";
    }

    return 0;
}