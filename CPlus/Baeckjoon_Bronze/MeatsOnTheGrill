#include <iostream>
#include <vector>

using namespace std;

int main()
{
	int T;
	cin >> T;

	for (int i = 0; i < T; i++)
	{
		int M, W;
		cin >> M >> W;
		
		for (int m = 0; m < M; m++)
		{
			string str;
			cin >> str;

			for (int n = str.size()-1; n >=0; n--)
			{
				cout << str[n];
			}
			cout << '\n';
		}
	}
}