#include <iostream>
#include <string>

using namespace std;

int main() 
{
    int T;
    cin >> T;

    for (int t = 0; t < T; ++t) 
    {
        int R;
        string S;
        cin >> R >> S;

        for (char ch : S) 
        {
            for (int i = 0; i < R; ++i) 
            {
                cout << ch;
            }
        }
        cout << '\n';
    }

    return 0;
}