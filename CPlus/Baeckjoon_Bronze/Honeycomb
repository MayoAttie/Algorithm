#include <iostream>
using namespace std;
int main()
{
    int n;
    cin >> n;

    int answer = 1;
    int max = 1;
    int add = 6;

    while (true)
    {
        if (n <= max)
            break;

        max += add;
        add += 6;
        answer++;
    }

    cout << answer << endl;
    return 0;
}