#include <iostream>
#include <vector>

using namespace std;

int main()
{
	int N;
	cin >> N;

	vector<int> arr(N);

	for (int i = 0; i < N; i++)
	{
		cin >> arr[i];
	}

	if (N == 1 && arr[0] == 0)
	{
		cout << "YES\n0";
	}
	else
	{
		cout << "YES\n";
		for (int i = 0; i < 3; i++)
			cout << arr[N-1];
	}

}