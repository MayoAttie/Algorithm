#include <iostream>
#include <string>
#include <vector>
#include <unordered_map>
#include <queue>
#include <algorithm>
#include <climits>

using namespace std;


int main()
{
    string str;
    cin >> str;

    int cnt[26] = {0};

    for (char ch : str)
    {
        cnt[ch - 'A']++;
    }

    cout << cnt[str[0] - 'A'];


    return 0;
}