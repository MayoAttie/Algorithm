#include <iostream>
#include <vector>
using namespace std;


int fibonacci(int num)
{
    if (num >= 2)
        return fibonacci(num - 1) + fibonacci(num - 2);
    else if (num == 1)
        return 1;
    else
        return 0;
}

int main()
{
	int N;
	cin >> N;


    int answer =fibonacci(N);

	cout << answer;
}