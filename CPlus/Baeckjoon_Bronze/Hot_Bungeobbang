#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
using namespace std;

int main() 
{
    int N, M;
    cin >> N >> M;

    vector<string> bread(N);

    for (int i = 0; i < N; ++i) 
    {
        cin >> bread[i];
    }

    for (int i = 0; i < N; ++i) 
    {
        reverse(bread[i].begin(), bread[i].end());
        cout << bread[i] << endl;
    }

    return 0;
}