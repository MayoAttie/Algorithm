#include <iostream>
#include <string>
#include <vector>
#include <unordered_map>
#include <queue>
#include <algorithm>
#include <climits>
using namespace std;


int main()
{
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    int n;
    cin >> n;

    while (n--)
    {
        long long num;
        cin >> num;
        if (num < 10)
            cout << 0;
        else
        {
            if (num % 10 == 0)
                cout << 1;
            else
                cout << 0;
        }
        cout << '\n';
    }


    return 0;
}