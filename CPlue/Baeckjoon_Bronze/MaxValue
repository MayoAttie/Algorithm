#include <iostream>
using namespace std;

int main() 
{
    int maxVal = -1; 
    int maxIdx = -1; 
    
    for (int i = 0; i < 9; i++) 
    {
        int num;
        cin >> num;
        
        if (num > maxVal) 
        { 
            maxVal = num;
            maxIdx = i + 1;
        }
    }

    cout << maxVal << endl;
    cout << maxIdx << endl;

    return 0;
}
